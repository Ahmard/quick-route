{
    "name": "ahmard/quick-route",
    "description": "Simple http router designed to look just like Laravel router",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Mustapha",
            "email": "ahmardy@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "nikic/fast-route": "^1.3",
        "opis/closure": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.4",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "QuickRoute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QuickRoute\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "phpstan analyse",
        "test": "phpunit"
    }
}
